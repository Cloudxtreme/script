    ;;
  dev07)
    ssh 172.18.75.85
    ;;
  dev10)
    ssh 172.18.75.150
    ;;
  dev11)
    ssh 172.18.75.250
    ;;
  staging)
    ssh 172.18.74.227
    ;;
  canary)
    ssh 172.18.74.222
    ;;
  aws)
    ssh 172.18.75.159
    ;;
